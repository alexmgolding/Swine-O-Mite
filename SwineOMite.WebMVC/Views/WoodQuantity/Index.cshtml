@model IEnumerable<SwineOMite.Models.WoodQuantity.WoodQuantityListItem>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-hover">
    <tr>
