            @Html.DisplayNameFor(model => model.WoodQuantityId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WoodAmount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.WoodQuantityId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WoodAmount)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.WoodQuantityId }) |
