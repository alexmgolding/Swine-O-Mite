@model SwineOMite.Models.SmookingWood.SmokingWoodCreate

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
<div class="form-horizontal">
    <h4>Smoking Wood Create</h4>
    <hr />

    <div class="form-group">
        @Html.LabelFor(model => model.WoodSpeciesType, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EnumDropDownListFor(model => model.WoodSpeciesType, htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.WoodSpeciesType, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.WoodVarietyType, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EnumDropDownListFor(model => model.WoodVarietyType, htmlAttributes: new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.WoodVarietyType, "", new { @class = "text-danger" })
        </div>
    </div>



    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
</div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
